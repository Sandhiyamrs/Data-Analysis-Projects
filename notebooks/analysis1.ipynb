import pandas as pd
import matplotlib.pyplot as plt

df = pd.read_csv('../datasets/dataset1.csv')
print(df.head())

print(df.describe())

plt.figure(figsize=(7,4))
plt.plot(df['date'], df['sales'])
plt.xticks(rotation=45)
plt.title("Daily Sales Trend")
plt.xlabel("Date")
plt.ylabel("Sales")
plt.show()

